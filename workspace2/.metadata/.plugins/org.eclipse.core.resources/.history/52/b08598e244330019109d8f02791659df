package com.decorate;

public class DecorateTest02 {

}

//抽象组件
interface Person{
	String getName(); 
	int getAge();
	
}

//具体组件
class Athletes implements Person{
	private String name;
	private int age;
	@Override
	public String getName() {
		return this.name;
	}

	@Override
	public int getAge() {
		return this.age;
	}
	
}
abstract class Skills{
	
}
